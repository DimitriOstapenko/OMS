<tr>
  <td><%= report.name %></td>
  <td><%= report.rtype %></td>
  <td><%= report.client_id %></td>
  <td><%= report.timeframe %></td>
  <td><%= report.sdate %></td>
  <td><%= report.edate %></td>
  <td><%= report.created_at %></td>
  <td><%= report.filename %></td>

  <% if current_user.admin? %>
       <td><%= link_to '<i class="glyphicon glyphicon-edit"></i>'.html_safe, edit_report_path(report) %> </td>
       <td><%= link_to '<i class="glyphicon glyphicon-remove-sign"></i>'.html_safe, report, method: :delete, data: { confirm: "Are you sure you want to delete #{report.name}?" } %> </td>
  <% end %>
 </tr>
